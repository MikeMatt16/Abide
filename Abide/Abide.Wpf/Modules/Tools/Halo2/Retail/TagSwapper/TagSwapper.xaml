<h2:ToolControl x:Class="Abide.Wpf.Modules.Tools.Halo2.Retail.TagSwapper.TagSwapper"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                xmlns:local="clr-namespace:Abide.Wpf.Modules.Tools.Halo2.Retail.TagSwapper"
                xmlns:h2="clr-namespace:Abide.AddOnApi.Wpf.Halo2;assembly=Abide.AddOnApi.Wpf"
                mc:Ignorable="d" 
                d:DesignHeight="450" d:DesignWidth="800"
                AddOnAuthor="Click16" AddOnDescription="Quick tag ID swapper"
                AddOnName="Tag Swapper"
                SelectedEntryChanged="ToolControl_SelectedEntryChanged"
                DataContextChanged="ToolControl_DataContextChanged">
    <h2:ToolControl.DataContext>
        <local:TagSwapperViewModel/>
    </h2:ToolControl.DataContext>
    <Grid>
        <ListView>
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Name" Width="300"/>
                    
                </GridView>
            </ListView.View>
            
        </ListView>
    </Grid>
</h2:ToolControl>
